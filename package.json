{
  "name": "docker-sharp-image-server",
  "version": "1.0.0",
  "description": "Docker container running node sharp and AWS S3 for image resizing",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "node index.js",
    "start:babel:prod": "nodemon node_modules/.bin/cross-env NODE_ENV=production babel-node index",
    "start:babel:dev": "nodemon node_modules/.bin/cross-env NODE_ENV=development babel-node index",
    "test": "npm run test:clean && ./script/test.server.sh",
    "test:unit": "./script/test.server.unit.sh",
    "test:clean": "rimraf coverage",
    "snyk": "snyk test",
    "slate": "rimraf node_modules && npm install",
    "ship": "./script/ship.sh"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:designreact/docker-sharp-image-server.git"
  },
  "author": "Martin King - DesignReact Ltd",
  "license": "MIT",
  "homepage": "https://github.com/designreact/docker-sharp-image-server",
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-istanbul": "^0.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-rewire": "^1.0.0-rc-4",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "babel-template": "^6.9.0",
    "babel-types": "^6.11.1",
    "cross-env": "^1.0.8",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "expect": "^1.20.1",
    "istanbul": "^0.4.3",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-istanbul": "^0.2.0",
    "mock-fs": "^3.9.0",
    "nock": "^8.0.0",
    "nodemon": "^1.11.0",
    "phantomjs-prebuilt": "2.1.7",
    "proxyquire": "^1.7.9",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "sinon-as-promised": "^4.0.0",
    "snyk": "^1.14.3",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "aws-sdk": "^2.20.0",
    "body-parser": "^1.15.1",
    "cache-manager": "^2.4.0",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "path": "^0.12.7",
    "request": "^2.72.0",
    "s3fs": "^2.5.0",
    "sharp": "0.15.0",
    "webpack": "^1.13.1"
  },
  "engines": ">=4.4.3"
}
